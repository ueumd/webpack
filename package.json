    "cross-env": "^5.2.0",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "rimraf": "^2.6.3",
    "style-loader": "^3.3.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
